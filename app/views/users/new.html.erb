<h2>Sign Up</h2>

<form action="<%= users_url %>" method="post">
  <%= form_auth_token %>

  <label for="username">Username
    <input type="text" name="user[username]">
  </label>

  <label for="password">Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Sign Up">
</form>
