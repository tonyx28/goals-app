<h2>Sign In</h2>

<form action="<%= session_url %>" method="post">
  <%= form_auth_token %>

  <label for="username">Username
    <input type="text" name="user[username]">
  </label>

  <label for="password">Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Sign In">
</form>
